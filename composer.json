{
    "name": "hosomikai/kotsms",
    "description": "Send sms use kotsms service - Laravel 5 version",
    "keywords": [
        "kotsms",
        "簡訊王",
        "sms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "hosomikai",
            "email": "showking111@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0|^8.0",
        "illuminate/support": "5.*|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Hosomikai\\Kotsms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hosomikai\\Kotsms\\KotsmsServiceProvider"
            ],
            "aliases": {
                "Kotsms": "Hosomikai\\Kotsms\\Facade"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev",
        "orchestra/testbench": "~3.6.7 || ~3.7.8 || ~3.8.6 || ^4.8 || ^5.2 || ^6.0"
    }
}